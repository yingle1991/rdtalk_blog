
page {
	height: 100%;
	width: 100%;
	background-color: #fdf1d2;
}
.model {
	/* position: absolute; */
	width: 100%;
	height: 100%;
	/* display: flex; */
	flex-direction: column;
	margin: 0 auto;
}
.whitePositionPanel {
	width: 100%;
	top: 0rpx;
	height: 48rpx;
	/* position: absolute; */
	/* background-color: #ffffff; */
	border-radius: 10rpx;
	display: flex;
	align-items: center;
	/* justify-content: center; */
	justify-content: space-between;
}
.top_black {
	height: 21%;
	position: relative;
}
.page-content {
	background-color: #EEEEEE;
	height: 100vh;
	padding: 20rpx;
}
.plate-number-view {
	background: #FFFFFF;
	border-radius: 12rpx;
	padding: 40rpx 0;
}
button {
	margin: 50rpx 0;
}
.swiper-content {
	position: relative;
	box-sizing: border-box;
	padding: 14rpx;
}
.caption {
	color: $uni-text-color-grey;
	font-size: $lee-font-size-caption;
	margin-bottom: $lee-spacing-base;
}
.action {
	float: right;
}

