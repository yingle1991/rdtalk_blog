  background: #fff;
}
.wrapper.data-v-cbd6070a {
  position: relative;
  z-index: 90;
  padding-bottom: 40rpx;
}
.wrapper .welcome.data-v-cbd6070a {
  position: relative;
  left: 50rpx;
  top: -90rpx;
  font-size: 46rpx;
  color: #555;
  text-shadow: 1px 0px 1px rgba(0, 0, 0, 0.3);
}
.back-btn.data-v-cbd6070a {
  position: absolute;
  left: 20rpx;
  top: calc(25px + 20rpx);
  z-index: 90;
  padding: 20rpx;
  font-size: 32rpx;
  color: #606266;
}
.left-top-sign.data-v-cbd6070a {
  font-size: 120rpx;
  color: #f8f8f8;
  position: relative;
  left: -12rpx;
}
/** 手机登录部分 */
.input-content.data-v-cbd6070a {
  padding: 0 60rpx;
}
.input-content .login-button.data-v-cbd6070a {
  margin-top: 30rpx;
}
.input-content .login-type.data-v-cbd6070a {
  display: flex;
  justify-content: flex-end;
